{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "start": { "outputs": [] },
    "lint": { "outputs": [] },
    "dev": { "cache": false },
    "clean": { "cache": false },
    "clean:cache": { "cache": false }
  }
}
